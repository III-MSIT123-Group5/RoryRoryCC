@model BussinessSystem_MVC.Models.CompanyVehicle
@if (Model.VehiclePhoto != null)
{
    <img src="@Url.Action("ShowPhoto", "CompanyVehicle", new {id=Model.LicenseNumber})" style="width:320px;height:240px" />
}
else
{
    <img src="@Url.Content("~/images/Noimage.png")" style="width:320px;height:240px" />
}
